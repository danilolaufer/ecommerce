<div class="container">
  <div style=" float:right;padding: 28px">
      <form action="http://localhost:8080/realtimeproducts">
      <button type="submit" class="btn btn-outline-success">Agregar Productos
      </button>
    </form>
  </div>
  <h1 style="text-align: center; color:red; font-weight:800; margin: 50px" class="mt-2">Lista de Productos </h1>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Descripcion</th>
        <th scope="col">Precio</th>
        <th scope="col">Stock</th>
        <th scope="col">Categoria</th>
        <th scope="col">CÃ³digo</th>
        <th scope="col">Imagen</th>
      </tr>
    </thead>

    <tbody>
      {{#each data}}
      <tr>

        <td>{{this.title}}</td>
        <td>{{description}}</td>
        <td>${{price}}</td>
        <td>{{stock}}</td>
        <td>{{category}}</td>
        <td>{{code}}</td>
        <td><img src="{{thumbnails}}" alt="" width="50%"> </td>
      </tr>

      {{/each}}
    </tbody>
  </table>




</div>